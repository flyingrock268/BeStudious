<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.beStudious"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application

        android:name="BeStudious"
        android:allowBackup="false"

        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ToDoList"
        tools:targetApi="31">


        <activity android:name=".TaskList.taskMain"
            android:screenOrientation="portrait"/>
        <activity android:name=".Calendar.CalendarMain"
            android:screenOrientation="portrait"/>
        <activity android:name=".Timer.TimerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".feed.feedMain"
            android:screenOrientation="portrait"/>
        <activity android:name=".Calendar.WeekViewActivity"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".Register"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Login"
            android:exported="true"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />

        </activity>

        <activity android:name=".SettingsActivity"
            android:screenOrientation="portrait">

        </activity>


    </application>

</manifest>